{
    "name": "destiny-ghost-api",
    "version": "2.4.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "start.js",
    "type": "module",
    "scripts": {
        "coverage": "npm run test:watch -- --coverage --run",
        "format": "remark . --quiet --frail --output",
        "lint": "eslint --fix --ignore-path .gitignore .",
        "start:production": "NODE_ENV=production npm start",
        "start:dev": "NODE_ENV=development npm start",
        "start": "node --require=suppress-experimental-warnings --loader=./loader.js --no-experimental-fetch -r dotenv/config --max-old-space-size=8192 --unhandled-rejections=strict start",
        "swagger": "swagger-jsdoc -d swaggerDef.cjs ./**/*.routes.js",
        "test": "npm run test:watch -- --run",
        "test:watch": "NODE_ENV=test node -r dotenv/config node_modules/vitest/vitest.mjs",
        "prepare": "husky || true"
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=v18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^4.0.0",
        "@godaddy/terminus": "^4.12.1",
        "@grpc/grpc-js": "^1.10.10",
        "@grpc/proto-loader": "^0.7.10",
        "@paralleldrive/cuid2": "^2.2.2",
        "@snyk/protect": "^1.1292.1",
        "agentkeepalive": "^4.5.0",
        "applicationinsights": "^3.2.0",
        "axios": "^1.6.4",
        "base64url": "^3.0.1",
        "better-sqlite3": "^11.1.2",
        "body-parser": "^1.20.2",
        "bullmq": "^5.8.3",
        "compression": "^1.7.4",
        "connect-redis": "^7.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.2",
        "express": "^4.18.2",
        "express-session": "^1.18.0",
        "graphql": "^16.9.0",
        "graphql-http": "^1.22.1",
        "helmet": "^7.1.0",
        "html-to-text": "^9.0.5",
        "http-status-codes": "^2.3.0",
        "ioredis": "^5.3.2",
        "is-my-json-valid": "^2.20.6",
        "joi": "^17.13.3",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.9.14",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^9.2.0",
        "pino-http": "^10.1.0",
        "qs": "^6.12.2",
        "rate-limiter-flexible": "^5.0.3",
        "redis-errors": "^1.2.0",
        "resolve": "^1.22.8",
        "rfc6902": "^5.1.1",
        "suppress-experimental-warnings": "^2.0.0",
        "swagger-ui-express": "^5.0.0",
        "twilio": "^5.2.2",
        "yauzl": "^3.1.3"
    },
    "overrides": {
        "nodemailer-smtp-transport": {
            "underscore": "^1.13.6"
        }
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^1.2.1",
        "chance": "^1.1.11",
        "eslint": "^8.54.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-security": "^1.7.1",
        "husky": "^9.0.11",
        "nock": "^13.4.0",
        "node-mocks-http": "^1.15.0",
        "pino-pretty": "^11.2.1",
        "remark-cli": "^12.0.0",
        "remark-lint-list-item-indent": "^4.0.0",
        "remark-preset-lint-consistent": "^6.0.0",
        "remark-preset-lint-recommended": "^7.0.0",
        "stream-json": "^1.8.0",
        "swagger-jsdoc": "^6.2.8",
        "v8": "^0.1.0",
        "vitest": "^1.2.1"
    },
    "snyk": true
}
