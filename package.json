{
    "name": "destiny-ghost-api",
    "version": "2.8.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "start.js",
    "type": "module",
    "scripts": {
        "coverage": "npm run test:watch -- --coverage --run",
        "format": "remark . --quiet --frail --output",
        "lint": "eslint --fix .",
        "start:production": "NODE_ENV=production npm start",
        "start:dev": "NODE_ENV=development npm start",
        "start": "node --require=suppress-experimental-warnings -r dotenv/config --max-old-space-size=8192 --unhandled-rejections=strict start.js",
        "swagger": "rm -f openapi.json && swagger-jsdoc -d openapi.cjs ./**/*.routes.js -o openapi.json",
        "test": "npm run test:watch -- --run",
        "test:watch": "NODE_ENV=test node -r dotenv/config node_modules/vitest/vitest.mjs",
        "prepare": "husky || true"
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=v22.0.0",
        "npm": ">=10.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^4.7.0",
        "@godaddy/terminus": "^4.12.1",
        "@google/genai": "^1.29.1",
        "@grpc/grpc-js": "^1.14.1",
        "@grpc/proto-loader": "^0.8.0",
        "@modelcontextprotocol/sdk": "^1.22.0",
        "@paralleldrive/cuid2": "^3.0.4",
        "@snyk/protect": "^1.1300.2",
        "agentkeepalive": "^4.6.0",
        "applicationinsights": "^3.12.0",
        "awesome-phonenumber": "^7.6.0",
        "axios": "^1.13.2",
        "base64url": "^3.0.1",
        "better-sqlite3": "^12.4.1",
        "bullmq": "^5.63.2",
        "compression": "^1.8.1",
        "connect-redis": "^9.0.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-session": "^1.18.2",
        "graphql": "^16.12.0",
        "graphql-http": "^1.22.4",
        "helmet": "^8.1.0",
        "html-to-text": "^9.0.5",
        "http-status-codes": "^2.3.0",
        "ioredis": "^5.8.2",
        "is-my-json-valid": "^2.20.6",
        "joi": "^18.0.1",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "lru-cache": "^11.2.2",
        "nodemailer": "^7.0.10",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^10.1.0",
        "pino-http": "^11.0.0",
        "qs": "^6.14.0",
        "rate-limiter-flexible": "^8.2.1",
        "redis": "^5.9.0",
        "resolve": "^1.22.11",
        "rfc6902": "^5.1.2",
        "suppress-experimental-warnings": "^2.0.0",
        "swagger-ui-express": "^5.0.0",
        "tinypool": "^2.0.0",
        "twilio": "^5.10.5",
        "yauzl": "^3.2.0",
        "zod": "^4.2.1"
    },
    "overrides": {
        "nodemailer-smtp-transport": {
            "underscore": "^1.13.6"
        }
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@vitest/coverage-v8": "^3.2.4",
        "chance": "^1.1.13",
        "eslint": "^9.39.1",
        "eslint-plugin-security": "^3.0.1",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "nock": "^14.0.10",
        "node-mocks-http": "^1.17.2",
        "pino-pretty": "^13.1.2",
        "remark-cli": "^12.0.0",
        "remark-lint-list-item-indent": "^4.0.1",
        "remark-preset-lint-consistent": "^6.0.1",
        "remark-preset-lint-recommended": "^7.0.1",
        "stream-json": "^1.9.1",
        "swagger-jsdoc": "^6.2.8",
        "v8": "^0.1.0",
        "vitest": "^3.2.4"
    },
    "snyk": true
}
