{
    "name": "destiny-ghost-api",
    "version": "2.4.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "start.js",
    "type": "module",
    "scripts": {
        "coverage": "npm run test:watch -- --coverage --run",
        "format": "remark . --quiet --frail --output",
        "lint": "eslint --fix --ignore-path .gitignore .",
        "start:production": "NODE_ENV=production npm start",
        "start:dev": "NODE_ENV=development npm start",
        "start": "node --require=suppress-experimental-warnings --loader=./loader.js --no-experimental-fetch -r dotenv/config --max-old-space-size=8192 --unhandled-rejections=strict start",
        "swagger": "swagger-jsdoc -d swaggerDef.cjs ./**/*.routes.js",
        "test": "npm run test:watch -- --run",
        "test:watch": "NODE_ENV=test node -r dotenv/config node_modules/vitest/vitest.mjs",
        "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=v18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^4.0.0",
        "@azure/service-bus": "^7.9.3",
        "@godaddy/terminus": "^4.12.1",
        "@grpc/grpc-js": "^1.9.14",
        "@grpc/proto-loader": "^0.7.10",
        "@paralleldrive/cuid2": "^2.2.2",
        "@snyk/protect": "^1.1271.0",
        "agentkeepalive": "^4.5.0",
        "applicationinsights": "^2.9.1",
        "axios": "^1.6.6",
        "base64url": "^3.0.1",
        "better-sqlite3": "^9.3.0",
        "body-parser": "^1.20.2",
        "compression": "^1.7.4",
        "connect-redis": "^7.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.2",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "helmet": "^7.1.0",
        "html-to-text": "^9.0.5",
        "http-status-codes": "^2.3.0",
        "ioredis": "^5.3.2",
        "is-my-json-valid": "^2.20.6",
        "joi": "^17.12.0",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.9.9",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^8.17.2",
        "pino-http": "^9.0.0",
        "qs": "^6.11.2",
        "rate-limiter-flexible": "^4.0.0",
        "redis-errors": "^1.2.0",
        "resolve": "^1.22.8",
        "rfc6902": "^5.1.1",
        "suppress-experimental-warnings": "^1.1.17",
        "swagger-ui-express": "^5.0.0",
        "twilio": "^4.20.0",
        "yauzl": "^2.10.0"
    },
    "overrides": {
        "nodemailer-smtp-transport": {
            "underscore": "^1.13.6"
        }
    },
    "devDependencies": {
        "@vitest/coverage-v8": "^1.2.1",
        "chance": "^1.1.11",
        "eslint": "^8.54.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-security": "^1.7.1",
        "husky": "^8.0.3",
        "nock": "^13.4.0",
        "node-mocks-http": "^1.14.0",
        "pino-pretty": "^10.3.1",
        "remark-cli": "^12.0.0",
        "remark-lint-list-item-indent": "^3.1.2",
        "remark-preset-lint-consistent": "^5.1.2",
        "remark-preset-lint-recommended": "^6.1.3",
        "stream-json": "^1.8.0",
        "swagger-jsdoc": "^6.2.8",
        "v8": "^0.1.0",
        "vitest": "^1.2.1"
    },
    "snyk": true
}
