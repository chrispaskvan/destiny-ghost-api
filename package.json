{
    "name": "destiny-ghost-api",
    "version": "2.3.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "start.js",
    "type": "module",
    "scripts": {
        "coverage": "npm run test:watch -- --coverage --run",
        "lint": "eslint --ignore-path .gitignore .",
        "start:production": "NODE_ENV=production npm start",
        "start:dev": "NODE_ENV=development npm start",
        "start": "node --loader=./loader.js --no-experimental-fetch -r dotenv/config --max-old-space-size=8192 --unhandled-rejections=strict start",
        "swagger": "swagger-jsdoc -d swaggerDef.cjs ./**/*.routes.js",
        "test": "npm run test:watch -- --run",
        "test:watch": "NODE_ENV=test node -r dotenv/config node_modules/vitest/vitest.mjs",
        "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=v18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^3.17.3",
        "@godaddy/terminus": "^4.11.2",
        "@paralleldrive/cuid2": "^2.2.0",
        "agentkeepalive": "^4.3.0",
        "applicationinsights": "^2.5.0",
        "axios": "^1.2.2",
        "azure-common": "^0.9.27",
        "azure-sb": "^0.11.2",
        "base64url": "^3.0.1",
        "better-sqlite3": "^8.2.0",
        "body-parser": "^1.20.1",
        "compression": "^1.7.4",
        "connect-redis": "^7.0.1",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^16.1.4",
        "express": "^4.18.2",
        "express-session": "^1.17.2",
        "helmet": "^6.0.1",
        "http-status-codes": "^2.1.4",
        "ioredis": "^5.2.5",
        "is-my-json-valid": "^2.20.6",
        "joi": "^17.9.1",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.9.0",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^8.8.0",
        "pino-http": "^8.3.1",
        "qs": "^6.11.1",
        "rate-limiter-flexible": "^2.4.1",
        "resolve": "^1.22.1",
        "rfc6902": "^5.0.1",
        "snyk": "^1.1127.0",
        "swagger-ui-express": "^4.6.2",
        "twilio": "^4.9.0",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@vitest/coverage-c8": "^0.30.1",
        "c8": "^7.12.0",
        "chance": "^1.1.11",
        "eslint": "^8.36.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.4",
        "eslint-plugin-security": "^1.7.1",
        "husky": "^8.0.3",
        "nock": "^13.3.0",
        "node-mocks-http": "^1.12.2",
        "pino-pretty": "^10.0.0",
        "swagger-jsdoc": "^6.2.8",
        "vitest": "^0.30.1"
    },
    "snyk": true
}
