{
    "name": "destiny-ghost-api",
    "version": "2.3.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "server.js",
    "scripts": {
        "coverage": "npm run test -- --coverage",
        "lint": "eslint --ignore-path .gitignore .",
        "start": "NODE_ENV=development node -r dotenv/config --trace-warnings server",
        "swagger": "swagger-jsdoc -d swaggerDef.js",
        "test": "NODE_ENV=test jest --config ./jest.config.js",
        "test:watch": "npm run test -- --watchAll"
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=16.11.1",
        "npm": "8.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "pre-push": "npm run test"
        }
    },
    "dependencies": {
        "@azure/cosmos": "^3.14.1",
        "@godaddy/terminus": "^4.9.0",
        "agentkeepalive": "^4.2.0",
        "applicationinsights": "^1.8.10",
        "axios": "^0.24.0",
        "azure-common": "^0.9.26",
        "azure-sb": "^0.11.2",
        "base64url": "^3.0.1",
        "better-sqlite3": "^7.4.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "connect-redis": "^4.0.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cuid": "^2.1.8",
        "dotenv": "^8.6.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "http-status-codes": "^2.1.4",
        "is-my-json-valid": "^2.20.6",
        "joi": "^17.5.0",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.6.3",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^6.13.1",
        "pino-http": "^6.5.0",
        "pino-pretty": "^4.7.1",
        "qs": "^6.10.1",
        "rate-limiter-flexible": "^2.2.4",
        "redis": "^3.1.1",
        "rfc6902": "^3.1.1",
        "snyk": "^1.828.0",
        "swagger-ui-express": "^4.1.6",
        "twilio": "^3.67.2",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "chance": "^1.1.7",
        "eslint": "^7.25.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-security": "^1.4.0",
        "husky": "^4.3.8",
        "jest": "^27.2.5",
        "node-mocks-http": "^1.10.1",
        "redis-mock": "^0.56.3",
        "swagger-jsdoc": "^4.3.2"
    },
    "snyk": true
}
