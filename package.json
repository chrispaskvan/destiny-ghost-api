{
    "name": "destiny-ghost-api",
    "version": "2.3.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "coverage": "npm run test:watch -- --coverage --run",
        "lint": "eslint --ignore-path .gitignore .",
        "start:production": "NODE_ENV=production npm start",
        "start:dev": "NODE_ENV=development npm start",
        "start": "node --experimental-specifier-resolution=node --no-experimental-fetch -r dotenv/config --trace-warnings --max-old-space-size=8192 --unhandled-rejections=strict server",
        "swagger": "swagger-jsdoc -d swaggerDef.js",
        "test": "npm run test:watch -- --run",
        "test:watch": "NODE_ENV=test node node_modules/vitest/vitest.mjs",
        "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=v18.0.0",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^3.17.1",
        "@godaddy/terminus": "^4.11.2",
        "agentkeepalive": "^4.2.1",
        "applicationinsights": "^2.3.5",
        "axios": "^0.27.2",
        "azure-common": "^0.9.27",
        "azure-sb": "^0.11.2",
        "base64url": "^3.0.1",
        "better-sqlite3": "^7.5.1",
        "body-parser": "^1.20.0",
        "compression": "^1.7.4",
        "connect-redis": "^6.1.3",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cuid": "^2.1.8",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "express-session": "^1.17.2",
        "http-status-codes": "^2.1.4",
        "ioredis": "^5.2.3",
        "is-my-json-valid": "^2.20.6",
        "joi": "^17.6.1",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.8.0",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^7.11.0",
        "pino-http": "^7.0.0",
        "qs": "^6.10.3",
        "rate-limiter-flexible": "^2.3.11",
        "rfc6902": "^5.0.1",
        "snyk": "^1.1012.0",
        "swagger-ui-express": "^4.1.6",
        "twilio": "^3.82.1",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "@vitest/coverage-c8": "^0.23.4",
        "c8": "^7.12.0",
        "chance": "^1.1.7",
        "eslint": "^8.24.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-security": "^1.5.0",
        "husky": "^7.0.4",
        "node-mocks-http": "^1.10.1",
        "pino-pretty": "^7.6.1",
        "redis-mock": "^0.56.3",
        "swagger-jsdoc": "^4.3.2",
        "vitest": "^0.23.4"
    },
    "snyk": true
}
