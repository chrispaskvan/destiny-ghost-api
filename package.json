{
    "name": "destiny-ghost-api",
    "version": "2.3.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "server.js",
    "scripts": {
        "coverage": "npm run test -- --coverage",
        "lint": "eslint --ignore-path .gitignore .",
        "start": "NODE_ENV=development DESTINY_DATABASE_DIR=./databases/destiny DESTINY2_DATABASE_DIR=./databases/destiny2 node server",
        "swagger": "swagger-jsdoc -d swaggerDef.js",
        "test": "NODE_ENV=test DESTINY_DATABASE_DIR=./databases/destiny DESTINY2_DATABASE_DIR=./databases/destiny2 jest --config ./jest.config.js",
        "test:watch": "npm run test -- --watchAll"
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=8.0.0",
        "npm": "5.4.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "pre-push": "npm run test"
        }
    },
    "dependencies": {
        "@azure/cosmos": "^3.7.4",
        "@godaddy/terminus": "^4.4.1",
        "@hapi/joi": "^17.1.1",
        "applicationinsights": "^1.8.5",
        "axios": "^0.19.2",
        "azure-common": "^0.9.25",
        "azure-sb": "^0.11.1",
        "base64url": "^3.0.1",
        "better-sqlite3": "^7.1.0",
        "body-parser": "^1.19.0",
        "connect-redis": "^4.0.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cuid": "^2.1.8",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "http-status-codes": "^1.4.0",
        "is-my-json-valid": "^2.20.5",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.20",
        "nodemailer": "^6.4.11",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^6.5.1",
        "pino-http": "^5.2.0",
        "qs": "^6.9.4",
        "rate-limiter-flexible": "^2.1.10",
        "redis": "^3.0.2",
        "rfc6902": "^3.1.1",
        "swagger-ui-express": "^4.1.4",
        "twilio": "^3.48.2",
        "yauzl": "^2.10.0",
        "snyk": "^1.370.1"
    },
    "devDependencies": {
        "chance": "^1.1.4",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-security": "^1.4.0",
        "husky": "^4.2.5",
        "jest": "^25.5.4",
        "node-mocks-http": "^1.8.1",
        "pino-pretty": "^3.6.1",
        "redis-mock": "^0.49.0",
        "swagger-jsdoc": "^4.0.0"
    },
    "snyk": true
}
