{
    "name": "destiny-ghost-api",
    "version": "2.7.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "start.js",
    "type": "module",
    "scripts": {
        "coverage": "npm run test:watch -- --coverage --run",
        "format": "remark . --quiet --frail --output",
        "lint": "eslint --fix .",
        "start:production": "NODE_ENV=production npm start",
        "start:dev": "NODE_ENV=development npm start",
        "start": "node --require=suppress-experimental-warnings --loader=./loader.js -r dotenv/config --max-old-space-size=8192 --unhandled-rejections=strict start",
        "swagger": "swagger-jsdoc -d openapi.cjs ./**/*.routes.js -o openapi.json",
        "test": "npm run test:watch -- --run",
        "test:watch": "NODE_ENV=test node -r dotenv/config node_modules/vitest/vitest.mjs",
        "prepare": "husky || true"
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=v22.0.0",
        "npm": ">=10.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^4.4.1",
        "@godaddy/terminus": "^4.12.1",
        "@google/genai": "^1.6.0",
        "@grpc/grpc-js": "^1.13.4",
        "@grpc/proto-loader": "^0.7.15",
        "@paralleldrive/cuid2": "^2.2.2",
        "@snyk/protect": "^1.1297.2",
        "agentkeepalive": "^4.6.0",
        "applicationinsights": "^3.7.0",
        "awesome-phonenumber": "^7.5.0",
        "axios": "^1.10.0",
        "base64url": "^3.0.1",
        "better-sqlite3": "^11.10.0",
        "bullmq": "^5.54.0",
        "compression": "^1.8.0",
        "connect-redis": "^9.0.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-session": "^1.18.1",
        "graphql": "^16.11.0",
        "graphql-http": "^1.22.4",
        "helmet": "^8.1.0",
        "html-to-text": "^9.0.5",
        "http-status-codes": "^2.3.0",
        "is-my-json-valid": "^2.20.6",
        "joi": "^17.13.3",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "nodemailer": "^7.0.3",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^9.7.0",
        "pino-http": "^10.5.0",
        "qs": "^6.14.0",
        "rate-limiter-flexible": "^7.1.1",
        "redis": "^5.5.6",
        "resolve": "^1.22.10",
        "rfc6902": "^5.1.2",
        "suppress-experimental-warnings": "^2.0.0",
        "swagger-ui-express": "^5.0.0",
        "tinypool": "^1.1.1",
        "twilio": "^5.7.1",
        "yauzl": "^3.2.0"
    },
    "overrides": {
        "nodemailer-smtp-transport": {
            "underscore": "^1.13.6"
        }
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@vitest/coverage-v8": "^3.2.3",
        "chance": "^1.1.13",
        "eslint": "^9.29.0",
        "eslint-plugin-security": "^3.0.1",
        "globals": "^16.2.0",
        "husky": "^9.1.7",
        "nock": "^14.0.5",
        "node-mocks-http": "^1.17.2",
        "pino-pretty": "^13.0.0",
        "remark-cli": "^12.0.0",
        "remark-lint-list-item-indent": "^4.0.1",
        "remark-preset-lint-consistent": "^6.0.1",
        "remark-preset-lint-recommended": "^7.0.1",
        "stream-json": "^1.9.1",
        "swagger-jsdoc": "^6.2.8",
        "v8": "^0.1.0",
        "vitest": "^3.2.3"
    },
    "snyk": true
}
