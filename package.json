{
    "name": "destiny-ghost-api",
    "version": "2.3.0",
    "description": "Node.js API for querying the Destiny database via SMS messages.",
    "main": "server.js",
    "scripts": {
        "coverage": "npm run test -- --coverage",
        "lint": "eslint --ignore-path .gitignore .",
        "start": "NODE_ENV=development node -r dotenv/config --trace-warnings server",
        "swagger": "swagger-jsdoc -d swaggerDef.js",
        "test": "NODE_ENV=test jest --config ./jest.config.js",
        "test:watch": "npm run test -- --watchAll",
        "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
    },
    "author": "Chris Paskvan",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/chrispaskvan/destiny-ghost-api"
    },
    "engines": {
        "node": ">=v16.11.1",
        "npm": ">=8.0.0"
    },
    "dependencies": {
        "@azure/cosmos": "^3.15.1",
        "@godaddy/terminus": "^4.10.2",
        "agentkeepalive": "^4.2.1",
        "applicationinsights": "^2.2.2",
        "axios": "^0.26.1",
        "azure-common": "^0.9.27",
        "azure-sb": "^0.11.2",
        "base64url": "^3.0.1",
        "better-sqlite3": "^7.5.0",
        "body-parser": "^1.19.2",
        "compression": "^1.7.4",
        "connect-redis": "^6.1.1",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cuid": "^2.1.8",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "express-session": "^1.17.2",
        "http-status-codes": "^2.1.4",
        "ioredis": "^4.28.5",
        "is-my-json-valid": "^2.20.6",
        "joi": "^17.6.0",
        "json-schema-defaults": "^0.4.0",
        "lodash": "^4.17.21",
        "nodemailer": "^6.7.2",
        "nodemailer-smtp-transport": "^2.7.4",
        "pino": "^7.8.1",
        "pino-http": "^6.6.0",
        "qs": "^6.10.3",
        "rate-limiter-flexible": "^2.2.4",
        "rfc6902": "^5.0.1",
        "snyk": "^1.868.0",
        "swagger-ui-express": "^4.1.6",
        "twilio": "^3.75.1",
        "yauzl": "^2.10.0"
    },
    "devDependencies": {
        "chance": "^1.1.7",
        "eslint": "^8.11.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-security": "^1.4.0",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "node-mocks-http": "^1.10.1",
        "pino-pretty": "^7.5.3",
        "redis-mock": "^0.56.3",
        "swagger-jsdoc": "^4.3.2"
    },
    "snyk": true
}
